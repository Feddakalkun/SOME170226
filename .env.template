# FANVUE OAUTH CONFIGURATION
# Or just enter them in the app: Settings → Fanvue → Connect
OAUTH_CLIENT_ID=your_client_id_here
OAUTH_CLIENT_SECRET=your_client_secret_here
OAUTH_REDIRECT_URI=http://localhost:3000/api/auth/callback
OAUTH_ISSUER_BASE_URL=https://auth.fanvue.com
FANVUE_APP_ID=your_app_id_here

# COMFYUI CONFIGURATION
COMFYUI_SERVER_ADDRESS=127.0.0.1:8188
COMFYUI_OUTPUT_PATH=C:/path/to/ComfyUI/output

# GENERATIVE AI (Optional for captions)
GOOGLE_GENERATIVE_AI_API_KEY=your_key_here

# APP SETTINGS
NEXT_PUBLIC_APP_NAME="Fanvue Creator Studio Pro"
NODE_ENV=development
